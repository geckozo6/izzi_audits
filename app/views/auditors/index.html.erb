<p id="notice"><%= notice %></p>

<h1>Auditors</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Name</th>
      <th>Bitrthday</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auditors.each do |auditor| %>
      <tr>
        <td><%= auditor.number %></td>
        <td><%= auditor.name %></td>
        <td><%= auditor.bitrthday %></td>
        <td><%= auditor.phone %></td>
        <td><%= link_to 'Show', auditor %></td>
        <td><%= link_to 'Edit', edit_auditor_path(auditor) %></td>
        <td><%= link_to 'Destroy', auditor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auditor', new_auditor_path %>
